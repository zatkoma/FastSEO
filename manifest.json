{
  "manifest_version": 2,
  "name": "Fast SEO - less clicks, better SEO",
  "short_name": "Fast SEO",
  "description": "Less clicks for better SEO. Don't copy domain and write site:domain.com in Google. By Martin Žatkovič",
  "version": "0.0.1.0",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "background": {"page": "background.html"},
  "browser_action": {
    "default_icon": "assets/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Fast SEO - Settings"
  }
}